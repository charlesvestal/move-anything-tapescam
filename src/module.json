        "step": 0.05
      },
      {
        "key": "color",
        "name": "Color",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.0,
        "step": 0.05
      },
      {
        "key": "wobble",
        "name": "Wobble",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.0,
        "step": 0.05
      },
      {
        "key": "tone",
        "name": "Tone",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.5,
        "step": 0.05
      },
      {
        "key": "output",
        "name": "Output",
        "type": "float",
