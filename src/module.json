{
  "id": "tapescam",
  "name": "TAPESCAM",
  "abbrev": "TS",
  "version": "0.5.0",
  "description": "Tape saturation and degradation effect",
  "author": "charlesvestal",
  "license": "MIT",
  "dsp": "tapescam.so",
  "api_version": 2,
  "capabilities": {
    "chainable": true,
    "component_type": "audio_fx",
    "ui_hierarchy": {
      "levels": {
        "root": {
          "name": "TAPESCAM",
          "params": [
            {
              "key": "input",
              "label": "Input",
              "type": "float",
              "min": 0.0,
              "max": 1.0,
              "default": 0.7,
              "step": 0.01,
              "unit": "%"
            },
            {
              "key": "drive",
              "label": "Drive",
              "type": "float",
              "min": 0.0,
              "max": 1.0,
              "default": 0.0,
              "step": 0.01,
              "unit": "%"
            },
            {
              "key": "color",
              "label": "Color",
              "type": "float",
              "min": 0.0,
              "max": 1.0,
              "default": 0.0,
              "step": 0.01,
              "unit": "%"
            },
            {
              "key": "wobble",
              "label": "Wobble",
              "type": "float",
              "min": 0.0,
              "max": 1.0,
              "default": 0.0,
              "step": 0.01,
              "unit": "%"
            },
            {
              "key": "noise",
              "label": "Noise",
              "type": "float",
              "min": 0.0,
              "max": 1.0,
              "default": 0.0,
              "step": 0.01,
              "unit": "%"
            },
            {
              "key": "tone",
              "label": "Tone",
              "type": "float",
              "min": 0.0,
              "max": 1.0,
              "default": 0.5,
              "step": 0.01,
              "unit": "%"
            },
            {
              "key": "output",
              "label": "Output",
              "type": "float",
              "min": 0.0,
              "max": 1.0,
              "default": 1.0,
              "step": 0.01,
              "unit": "%"
            },
            {
              "key": "age",
              "label": "Age",
              "type": "enum",
              "options": [
                "NEW",
                "USED",
                "WORN"
              ],
              "default": "NEW"
            },
            {
              "key": "speed",
              "label": "Speed",
              "type": "enum",
              "options": [
                "HIGH",
                "STD",
                "LOW"
              ],
              "default": "HIGH"
            },
            {
              "key": "compression",
              "label": "Comp",
              "type": "enum",
              "options": [
                "OFF",
                "LITE",
                "HEAVY"
              ],
              "default": "OFF"
            },
            {
              "key": "widen",
              "label": "Widen",
              "type": "enum",
              "options": [
                "OFF",
                "ON"
              ],
              "default": "ON"
            }
          ],
          "knobs": [
            "drive",
            "color",
            "wobble",
            "noise",
            "tone",
            "output",
            "age",
            "compression"
          ]
        }
      }
    }
  }
}
